        <!-- partial -->

        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->

            <!--sidebar-->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item nav-profile">
                        <a href="#" class="nav-link">
                            <div class="nav-profile-image">
                                <img src="/images/faces/face1.jpg" alt="profile">
                                <span class="login-status online"></span>
                                <!--change to offline or busy as needed-->
                            </div>
                            <div class="nav-profile-text d-flex flex-column">
                                <span class="font-weight-bold mb-2">David Grey. H</span>
                                <span class="text-secondary text-small">Project Manager</span>
                            </div>
                            <i class="mdi mdi-bookmark-check text-success nav-profile-badge"></i>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/appHome">
                            <span class="menu-title">Inicio</span>
                            <i class="mdi mdi-home menu-icon"></i>
                        </a>
                    </li>

                    <!-- Menú cuentas -->
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#ui-basic" aria-expanded="false"
                            aria-controls="ui-basic">
                            <span class="menu-title">Cuentas</span>
                            <i class="menu-arrow"></i>
                            <i class="mdi mdi-cash-multiple menu-icon"></i>
                        </a>
                        <div class="collapse" id="ui-basic">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link"
                                        href="pages/samples/cuentasEfectivo.html">Banco</a></li>
                                <li class="nav-item"> <a class="nav-link"
                                        href="pages/ui-features/typography.html">Tarjeta</a></li>
                                <li class="nav-item"> <a class="nav-link"
                                        href="pages/ui-features/typography.html">Efectivo</a></li>
                            </ul>
                            <button class="btn btn-block btn-sm btn-gradient-primary mt-4">+ Agregar cuenta</button>
                        </div>
                    </li>
                    <!--Fin lista de cuentas -->
                </ul>
            </nav>
            <!-- fin sidebar -->
            <!-- partial -->

            <div class="main-panel">
                <div class="form-check">
                    <label class="form-check-label">
                        <input type="checkbox" class="form-check-input" checked>
                        ¡Has dado el primer paso para la administración de tu dinero!
                        <i class="input-helper"></i>
                    </label>
                </div>

                <div class="page-header">
                    <h3 class="page-title">
                        <span class="page-title-icon bg-gradient-primary text-white mr-2">
                            <i class="mdi mdi-home"></i>
                        </span> Inicio </h3>
                    <nav aria-label="breadcrumb">
                        <ul class="breadcrumb">
                            <li class="breadcrumb-item active" aria-current="page">
                                <span></span>Mis cuentas <i
                                    class="mdi mdi-cash-multiple icon-sm text-primary align-middle"></i>
                            </li>
                        </ul>
                    </nav>
                </div>
                <!--Div resúmen de cuentas-->
                <div class="row ">
                    <!-- Cuenta-->
                    <div class="col-md-4 stretch-card grid-margin">
                        <div class="card bg-gradient-danger card-img-holder text-white">
                            <div class="card-body">
                                <img src="/images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                                <h4 class="font-weight-normal mb-3">Cuenta <i
                                        class="mdi mdi-chart-line mdi-24px float-right"></i>
                                </h4>
                                <h2 class="mb-5">$ 0.00</h2>
                                <h6 class="card-text">Ahorros 0%</h6>
                            </div>
                        </div>
                    </div>
                    <!--Fin tarjeta de cuenta-->

                    <!-- Formularios para crear nueva cuenta-->
                    <div class="col-md-6 grid-margin stretch-card">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Crea tu primera cuenta.</h4>
                                <p class="card-description"> Ingresa la cuenta que quieras gestionar, por ejemplo la
                                    cuenta de tu tarjeta de credito. </p>

                                <div class="form-group">
                                    <label for="exampleInputName1">Nombre de la cuenta:</label>
                                    <input type="text" class="form-control" id="exampleInputName1"
                                        placeholder="Tarjeta de Crédito 1">
                                </div>

                                <div class="form-group">
                                    <label for="exampleInputName1">Cantidad inicial de la cuenta:</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text bg-gradient-primary text-white">L</span>
                                        </div>
                                        <input type="text" class="form-control" placeholder="10,000.00"
                                            aria-label="Amount (to the nearest dollar)">
                                        <div class="input-group-append">
                                            <span class="input-group-text">.00</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="exampleInputName1">Si deseas puedes ahorrar una cantidad de dinero al
                                        mes, ingresala:</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text bg-gradient-primary text-white">L</span>
                                        </div>
                                        <input type="text" class="form-control" placeholder="2,000.00"
                                            aria-label="Amount (to the nearest dollar)">
                                        <div class="input-group-append">
                                            <span class="input-group-text">.00</span>
                                        </div>
                                    </div>
                                </div>
                                <button class="btn btn-block btn-sm btn-gradient-primary mt-4">+ Agregar cuenta</button>
                            </div>
                        </div>
                        <!-- Fin del formularios para crear nueva cuenta-->

                    </div>

                </div>
                <!--Fin div resúmenm de cuentas-->
